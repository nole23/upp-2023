      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.zahtjevZaRegistraciju && $data.zahtjevZaRegistraciju_ponuda && $data.zahtjevZaRegistraciju_klijent ){",
        "\t//attach lazy references variables to parent variables",
        "\t$data.zahtjevZaRegistraciju.ponuda = $data.zahtjevZaRegistraciju_ponuda;",
        "\t$data.zahtjevZaRegistraciju.klijent = $data.zahtjevZaRegistraciju_klijent;",
        "\treturn {",
        "\t\t//map zahtjevZaRegistraciju variable to expected task contract input",
        "\t\tzahtjevZaRegistracijuInput: {",
        "\t\t\tponuda: $data.zahtjevZaRegistraciju.ponuda.map( it => ({",
        "\t\t\t\tpersistenceId_string: it.persistenceId_string !== undefined ? it.persistenceId_string : null,",
        "\t\t\t\todabranaUsluga: it.odabranaUsluga !== undefined ? it.odabranaUsluga : null",
        "\t\t\t}))",
        "\t\t}",
        "\t}",
        "}"
