        "\t$data.zahtjevZaRegistraciju.ponuda = $data.zahtjevZaRegistraciju_ponuda;",
        "\t$data.zahtjevZaRegistraciju.klijent = $data.zahtjevZaRegistraciju_klijent;",
        "\t$data.zahtjevZaRegistraciju.ugovor = $data.zahtjevZaRegistraciju_ugovor;",
        "\treturn {",
        "\t\t//map zahtjevZaRegistraciju variable to expected task contract input",
        "\t\tzahtjevZaRegistracijuInput: {",
        "\t\t\tponuda: $data.zahtjevZaRegistraciju.ponuda.map( it => ({",
        "\t\t\t\tpersistenceId_string: it.persistenceId_string !== undefined ? it.persistenceId_string : null,",
        "\t\t\t\tnaziv: it.naziv !== undefined ? it.naziv : null,",
        "\t\t\t\tcijena: it.cijena !== undefined ? it.cijena : null",
        "\t\t\t}))",
        "\t\t}",
        "\t}",
        "}"
      ],
