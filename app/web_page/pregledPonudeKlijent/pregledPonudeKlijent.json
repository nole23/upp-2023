    "zahtjevZaRegistraciju_klijent" : {
      "type" : "url",
      "value" : [
        "{{zahtjevZaRegistraciju|lazyRef:'klijent'}}"
      ],
      "exposed" : false
    },
    "submit_errors_list" : {
      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.zahtjevZaRegistraciju && $data.zahtjevZaRegistraciju_ponuda && $data.zahtjevZaRegistraciju_klijent ){",
        "\t//attach lazy references variables to parent variables",
        "\t$data.zahtjevZaRegistraciju.ponuda = $data.zahtjevZaRegistraciju_ponuda;",
        "\t$data.zahtjevZaRegistraciju.klijent = $data.zahtjevZaRegistraciju_klijent;",
        "\treturn {",
        "\t\t//map zahtjevZaRegistraciju variable to expected task contract input",
        "\t\tzahtjevZaRegistracijuInput: {",
        "\t\t\tponuda: $data.zahtjevZaRegistraciju.ponuda.map( it => ({",
        "\t\t\t\tpersistenceId_string: it.persistenceId_string !== undefined ? it.persistenceId_string : null",
        "\t\t\t})),",
        "\t\t\tstatus: $data.zahtjevZaRegistraciju.status !== undefined ? $data.zahtjevZaRegistraciju.status : null,",
        "\t\t\tklijent: $data.zahtjevZaRegistraciju.klijent ? {",
        "\t\t\t\tpersistenceId_string: $data.zahtjevZaRegistraciju.klijent.persistenceId_string !== undefined ? $data.zahtjevZaRegistraciju.klijent.persistenceId_string : null",
        "\t\t\t} : null",
        "\t\t}",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "context" : {
      "type" : "url",
      "value" : [
        "../API/bpm/userTask/{{taskId}}/context"
      ],
      "exposed" : false
    },
    "ponuda_query" : {
      "type" : "url",
      "value" : [
        "../API/bdm/businessData/com.company.model.Ponuda?q=find&p=0&c=99"
      ],
      "exposed" : false
    },
    "klijent_query" : {
      "type" : "url",
      "value" : [
        "../API/bdm/businessData/com.company.model.Klijent?q=find&p=0&c=99"
      ],
      "exposed" : false
    },
    "taskId" : {
      "type" : "urlparameter",
      "value" : [
        "id"
      ],
      "exposed" : false
    },
    "opcijeStatusa" : {
      "type" : "expression",
      "value" : [
        "return [\"NASTAVITI\", \"ODUSTATI\"]"
      ],
      "exposed" : false
    },
    "setovanjeTabele" : {
      "type" : "expression",
      "value" : [
        "",
        "var ponuda =  $data.zahtjevZaRegistraciju_ponuda;",
        "",
        "var listaObjekata = [];",
        "var sum = parseInt(0);",
        "ponuda.forEach((item) => {",
        "   listaObjekata.push({Id: item.persistenceId, Naziv: item.naziv, Cijena: item.cijena}); ",
        "   ",
        "   sum += parseInt(item.cijena);",
        "});",
        "",
        "return {",
        "    lista: listaObjekata,",
        "    mjesecnaCijena: sum,",
        "    godisnjaCijena: sum * 12",
        "};"
      ],
      "exposed" : false
    }
  },
